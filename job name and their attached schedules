USE msdb;
GO

SELECT 
    j.name AS JobName,
    s.name AS ScheduleName,
    s.enabled AS ScheduleEnabled,
    s.freq_type,
    s.active_start_time
FROM 
    dbo.sysjobschedules js
INNER JOIN 
    dbo.sysjobs j ON js.job_id = j.job_id
INNER JOIN 
    dbo.sysschedules s ON js.schedule_id = s.schedule_id
ORDER BY 
    j.name;
